		<parameter name="STR_RECEIPT_ID" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[select rec.RECEIPT_ID,
DATE_FORMAT(rec.RECEIPT_DATE, '%d-%b-%Y') as RECEIPT_DATE,
cust.CUSTOMER_NAME,
rec.RECEIVED_AMT,
rec.CLOSING_BAL 
from TABLE_RECEIPT rec,
CUSTOMER_TBL cust 
where rec.CUSTOMER_ID=cust.CUSTOMER_ID
and RECEIPT_ID=$P{STR_RECEIPT_ID}]]>
		</queryString>
